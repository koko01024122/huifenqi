<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_express_finish"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context="com.example.cxhll.huifenq.Activitys.ExpressFinishActivity">
<include layout="@layout/actionbar"
    android:id="@+id/aef_bar"/>
    <RelativeLayout
        android:layout_below="@+id/aef_bar"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <ProgressBar
            android:id="@+id/aef_laoding"
            android:layout_width="match_parent"
            android:layout_height="200dp"

            android:textDirection="firstStrong"
            android:textAlignment="center"
            />
        <LinearLayout
            android:orientation="vertical"
            android:id="@+id/aef_pass"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="200dp">
            <TextView
                android:id="@+id/send_state"
                android:layout_marginTop="80dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="资料提交成功"
                android:gravity="center"

                android:textSize="30sp"
                android:textStyle="bold"
                android:textColor="@color/colorPrimary"/>
            <TextView
                android:id="@+id/send_reason"
                android:layout_marginTop="10dp"
                android:layout_width="match_parent"
                android:layout_height="50dp"

                android:text="本页面将在3秒后关闭"
                android:gravity="center"
                android:textSize="15sp"

                android:textColor="@color/colorPrimary"/>
        </LinearLayout>
    </RelativeLayout>
</RelativeLayout>
